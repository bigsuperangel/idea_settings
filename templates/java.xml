<templateSet group="java模板">
  <template name="ps" value="private String $END$;" description="新建string字段" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="psc" value="/** $VAR1$ **/&#10;private String $VAR2$;" description="新建string字段带注释" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pi" value="private Integer $END$;" description="新建Integer类型字段" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pic" value="/** $VAR1$ **/&#10;private Integer $VAR2$;" description="新建Integer字段带注释" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pl" value="private Long $END$;" description="新建Long类型字段" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="plc" value="/** $VAR1$ **/&#10;private Long $VAR2$;" description="新建Long字段带注释" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="logerr" value="log.error(StrUtil.format($VAR1$,$VAR2$));" description="输出格式化后的错误日志" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="nlog" value="private static final Log log = Log.getLog($CLASS_NAME$.class);&#10;    $END$" description="新建一个log" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="loginfo" value="log.info(StrUtil.format($VAR1$,$VAR2$));" description="输出格式化后的日志" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="jps" value="private String $UField$$END$ = &quot;$VAR1$&quot;;&#10;&#10;public String get$Field$(){&#10;    return get($UField$);&#10;}&#10;&#10;public $CLASS$ set$Field$(String value){&#10;    set($UField$,value);&#10;    return this;&#10;}" description="jfinal新建字段" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="UField" expression="capitalize(VAR1)" defaultValue="capitalize(VAR1)" alwaysStopAt="false" />
    <variable name="Field" expression="" defaultValue="capitalize(camelCase(VAR1))" alwaysStopAt="true" />
    <variable name="CLASS" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>